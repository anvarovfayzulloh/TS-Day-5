{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "dist/**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/dist/css/$1" },
    { "src": "/js/(.*)", "dest": "/dist/js/$1" }
  ]
}
